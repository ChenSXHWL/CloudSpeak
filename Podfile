platform :ios, ‘8.0’

inhibit_all_warnings!

def pods
    pod 'Masonry'
    pod 'EasyIOS', :git=>'https://github.com/zhuchaowe/EasyIOS.git', :tag=>’2.3’
    pod 'EasyIOS-Extention', '~> 1.2'
    pod 'XAspect'
    pod 'IQKeyboardManager'
    pod 'SDCycleScrollView','~> 1.65'
    pod 'SZTextView'
    pod 'MJRefresh'
    pod 'Qiniu', '~> 7.1'
    pod 'DGActivityIndicatorView'
    
    pod 'LBXScan/LBXNative','~> 2.0'
    pod 'LBXScan/LBXZXing','~> 2.0'
    pod 'LBXScan/LBXZBar','~> 2.0'
    pod 'LBXScan/UI','~> 2.0'
    
    # 集成极光推送
    pod 'JPush'
    # 集成微信(精简版0.2M)
    pod 'UMengUShare/Social/ReducedWeChat'
    
    # 集成微信(完整版14.4M)
    pod 'UMengUShare/Social/WeChat'
    
    # 集成QQ(精简版0.5M)
    pod 'UMengUShare/Social/ReducedQQ'
    
    # 集成QQ(完整版7.6M)
    pod 'UMengUShare/Social/QQ'
    
    # 集成短信
    pod 'UMengUShare/Social/SMS'
    
    pod 'AMap3DMap'  #3D地图 SDK
    pod 'AMapSearch'  #地图 SDK 搜索功能
    pod 'ServerLog', :git => 'https://github.com/Darkhorse-Fraternity/TestLog.git',:configurations => ['Debug']
end

target ‘CloudSpeak’ do
    pods
end
